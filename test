        <li class="control">
            <input type="radio" class="radio validation-passed" onclick="$('shipping:same_as_billing').checked = false;" title="Ship to different address" value="0" id="billing:use_for_shipping_no" name="billing[use_for_shipping]" /><label for="billing:use_for_shipping_no">Ship to different address</label>
        </li>
